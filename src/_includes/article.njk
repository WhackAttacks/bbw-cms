---
layout: 'base.njk'
---
    <main>
       
<article class="main-article">
<div class="article-con">
  
    <h1 class="article-title"> {{ title }} </h1>
    
    <div class="art-img">
      <figure class="main-article__figure">
      <img class="fig-img" src="{{ image }}" alt="{{ imageAlt }}">
      <figcaption>
        {{ imageAlt }}
      </figcaption>
    </figure>
    </div>
  <div class="texts-article">
    {{ content | safe }}
  </div>
</div>
</article>
    </main>